{
  "presets": [
      "@babel/preset-react",
      ["@babel/preset-env", {
          "targets": {
            "esmodules": true,
             "chrome": "90"
          },
          "modules": false
      }]
  ],
  "plugins": [
        "@babel/plugin-transform-async-to-generator",
        ["@babel/plugin-proposal-decorators", { "legacy": true  }],
        ["@babel/plugin-proposal-class-properties", { "loose": true }],
        ["@babel/plugin-transform-classes", { "loose": true }]
  ]
}
